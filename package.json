    "eslint-plugin-vue": "^7.0.1",
    "firebase-tools": "^8.12.1",
    "jest": "^26.4.2",
    "less": "^4.1.1",
    "node-sass": "^5.0.0",
    "postcss": "^7",
    "prettier": "^2.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylus": "^0.54.8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-jest": "^26.4.1",
    "typescript": "^4.1.3",
